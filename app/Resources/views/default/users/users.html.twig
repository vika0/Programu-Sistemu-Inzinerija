
{% extends "::base.html.twig" %}

{% block body %}
    <div id="project-fpage" class="container">
        <div class="row">
            <div class="text-center">
                <h1>Vartotojai</h1>
            </div>
            <table class="table table-sm table-inverse">
                <thead>
                <tr>
                    <th>Vardas</th>
                    <th>Pavardė</th>
                    <th>Gimimo data</th>
                    <th>Asmens kodas</th>
                    <th>El. paštas</th>
                    <th>Telefono nr.</th>
                </tr>
                </thead>
                <tbody>
                {% for val in users %}
                <tr>
                    <td>{{ val.name }}</td>
                    <td>{{ val.surname }}</td>
                    <td>{{ val.dateOfBirth|date('Y-m-d') }}</td>
                    <td>{{ val.personalId }}</td>
                    <td>{{ val.email }}</td>
                    <td>{{ val.phone }}</td>
                </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>

{% endblock %}